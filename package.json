{
  "name": "safetruck-front",
  "version": "1.0.1",
  "description": "Safetruck App",
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:dev",
    "ci": "npm run lint && npm test && npm run e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules dist",
    "github-deploy:dev": "webpack --config config/webpack.github-deploy.js --progress --profile --github-dev",
    "github-deploy:prod": "webpack --config config/webpack.github-deploy.js --progress --profile --github-prod",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "postversion": "git push && git push --tags",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "preversion": "npm test",
    "rimraf": "rimraf",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "lite-server -c=bs-config.json",
    "server": "npm run server:dev",
    "show:stats": "concurrently \"npm run show:stats:app\" \"npm run show:stats:vendor\"",
    "show:stats:app": "source-map-explorer dist/main.bundle.js",
    "show:stats:vendor": "source-map-explorer dist/vendor.bundle.js",
    "start:hmr": "npm run server:dev:hmr",
    "start": "npm run server:dev",
    "tslint": "tslint --fix --format verbose",
    "typedoc": "typedoc",
    "version": "npm run build",
    "watch:dev:hmr": "npm run watch:dev -- --hot",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "commitmsg": "validate-commit-msg"
  },
  "config": {
    "validate-commit-msg": {
      "types": [
        "feat",
        "fix",
        "docs",
        "style",
        "refactor",
        "perf",
        "test",
        "chore",
        "revert",
        "ci",
        "build"
      ],
      "warnOnFail": false,
      "maxSubjectLength": 100,
      "subjectPattern": ".+",
      "subjectPatternErrorMsg": "subject does not match subject pattern!",
      "helpMessage": ""
    },
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@angular/common": "^2.4.8",
    "@angular/compiler": "^2.4.8",
    "@angular/core": "^2.4.8",
    "@angular/forms": "^2.4.8",
    "@angular/http": "^2.4.8",
    "@angular/platform-browser": "^2.4.8",
    "@angular/platform-browser-dynamic": "^2.4.8",
    "@angular/platform-server": "^2.4.8",
    "@angular/router": "^3.4.8",
    "@angularclass/conventions-loader": "^1.0.13",
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "@angularclass/request-idle-callback": "^1.0.7",
    "@angularclass/webpack-toolkit": "^1.3.3",
    "@reactivex/rxjs": "^5.2.0",
    "@types/eonasdan-bootstrap-datetimepicker": "^4.17.22",
    "angular2-autosize": "^1.0.1",
    "angular2-datatable": "^0.5.3",
    "angular2-text-mask": "^4.1.0",
    "animate.css": "^3.5.2",
    "assets-webpack-plugin": "^3.5.1",
    "bootstrap": "4.0.0-alpha.4",
    "core-js": "^2.4.1",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "font-awesome": "^4.7.0",
    "glyphicons-halflings": "^1.9.0",
    "ie-shim": "^0.1.0",
    "jquery": "^3.1.1",
    "jquery-slimscroll": "^1.3.8",
    "jwt-decode": "^2.1.0",
    "lodash": "^4.17.4",
    "messenger": "git+https://github.com/HubSpot/messenger.git#v1.4.2",
    "moment": "^2.17.1",
    "ng2-bootstrap": "^1.3.3",
    "ng2-breadcrumb": "^0.5.6",
    "ng2-table": "^1.3.2",
    "pace": "git+https://github.com/HubSpot/pace.git#v0.7.7",
    "sass-resources-loader": "^1.2.0",
    "widgster": "^0.0.3",
    "zone.js": "^0.7.7"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.35",
    "@types/google-maps": "^3.2.0",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.43",
    "@types/lodash": "^4.14.53",
    "@types/node": "^7.0.5",
    "@types/source-map": "^0.5.0",
    "@types/uglify-js": "^2.6.28",
    "@types/webpack": "^2.2.6",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.0.4",
    "chalk": "^1.1.3",
    "codelyzer": "^2.0.1",
    "compression-webpack-plugin": "^0.3.2",
    "concurrently": "^3.4.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "dotenv": "^4.0.0",
    "exports-loader": "^0.6.3",
    "expose-loader": "^0.7.3",
    "file-loader": "^0.10.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.1",
    "imports-loader": "^0.7.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "json-loader": "^0.5.4",
    "lite-server": "^2.2.2",
    "metrojs": "^0.9.77",
    "node-sass": "^4.5.0",
    "parse5": "^3.0.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.0",
    "sass-loader": "^6.0.1",
    "source-map-explorer": "^1.3.3",
    "source-map-loader": "^0.1.6",
    "standard": "^8.6.0",
    "string-replace-loader": "^1.0.5",
    "style-loader": "^0.13.1",
    "to-string-loader": "^1.1.5",
    "ts-helpers": "^1.1.2",
    "ts-node": "^2.1.0",
    "tslint": "^4.4.2",
    "tslint-config-standard": "^4.0.0",
    "tslint-loader": "^3.4.2",
    "typedoc": "^0.5.7",
    "typescript": "^2.2.1",
    "url-loader": "^0.5.7",
    "validate-commit-msg": "^2.11.1",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.1",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^3.0.0"
  },
  "overrides": {
    "rickshaw": {
      "dependencies": {
        "d3": "3.5.5"
      }
    }
  },
  "engines": {
    "node": ">= 4.2.1",
    "npm": ">= 3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xdevelsistemas/safetruck-front.git"
  },
  "bugs": {
    "url": "https://github.com/xdevelsistemas/safetruck-front/issues"
  },
  "homepage": "https://github.com/xdevelsistemas/safetruck-front#readme"
}
